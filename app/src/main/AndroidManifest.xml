<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.MobMaster">
    <uses-sdk android:targetSdkVersion="24" android:minSdkVersion="19"></uses-sdk>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_mobmaster"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity
            android:name="com.example.MobMaster.SplashScreen"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.MobMaster.AppBase"
            android:parentActivityName="com.example.MobMaster.SplashScreen">
        </activity>
        <activity
            android:name="com.example.MobMaster.profile_activity"
            android:parentActivityName="com.example.MobMaster.AppBase" />
        <activity
            android:name="com.example.MobMaster.Student_Registartion"
            android:parentActivityName="com.example.MobMaster.profile_activity" />
        <activity
            android:name="com.example.MobMaster.noteCreate"
            android:parentActivityName="com.example.MobMaster.noteActivity" />
        <activity
            android:name="com.example.MobMaster.attendanceActivity"
            android:parentActivityName="com.example.MobMaster.AppBase" />
        <activity
            android:name="com.example.MobMaster.Result_activity"
            android:parentActivityName="com.example.MobMaster.cgpa_activity" />
        <activity
            android:name="com.example.MobMaster.cgpa_activity"
            android:parentActivityName="com.example.MobMaster.AppBase" />
        <activity
            android:name="com.example.MobMaster.noteActivity"
            android:parentActivityName="com.example.MobMaster.AppBase" />
        <activity
            android:name="com.example.MobMaster.scheduler"
            android:parentActivityName="com.example.MobMaster.AppBase" />
        <activity
            android:name="com.example.MobMaster.make_schedule"
            android:parentActivityName="com.example.MobMaster.scheduler" />
        <activity
            android:name="com.example.MobMaster.SettingsActivity"
            android:label="@string/title_activity_settings"
            android:parentActivityName="com.example.MobMaster.AppBase" />
        <activity
            android:name="com.example.MobMaster.About"
            android:parentActivityName="com.example.MobMaster.AppBase" />
        <activity
            android:name="com.example.MobMaster.Edit_Student"
            android:parentActivityName="com.example.MobMaster.AppBase" />
        <activity
            android:name="com.example.MobMaster.ocr"
            android:parentActivityName="com.example.MobMaster.AppBase" />
    </application>

</manifest>